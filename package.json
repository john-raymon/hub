{
  "name": "hub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "shopify-buy": "^2.0.1",
    "velocity-animate": "^1.5.2",
    "velocity-react": "^1.4.1"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/styles/application.scss --include-path ./node_modules src/styles/application.scss -o src/styles/",
    "watch-css": "yarn build-css && node-sass --include-path ./src/styles/application.scss --include-path ./node_modules src/styles/application.scss -o src/styles/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "add": "^2.0.6",
    "basement": "https://github.com/sanctuarycomputer/basement.git",
    "yarn": "^1.13.0"
  }
}
